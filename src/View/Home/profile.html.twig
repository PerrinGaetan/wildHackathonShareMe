{% extends "layout.html.twig" %}
{% block stylesheet %}
	{{ parent() }}
	<link rel="stylesheet" href="assets/css/styleProfile.css">
{% endblock %}
{% block content %}

	<header>
		<div class="overlay">
			<h1>Ne cherchez plus,<br/>C'est ici que vous trouverez.</h1><br>
			<a href="/">
				<button>Accueil</button>
			</a>
		</div>
	</header>
	
	<form action="" method="post" class="form__addAlbum">
		<div class="form__group">
			<input type="text" class="form__input" id="name" placeholder="Artiste..." required=""/>
			<label for="name" class="form__label"></label>
		</div>
		<div class="form__group">
			<input type="text" class="form__input" id="name" placeholder="Album..." required=""/>
			<label for="name" class="form__label"></label>
		</div>
		<input class="send" type="submit" value="Ajouter Ã  ma liste">
	</form>

	<section class="list">
		{% if results %}
			<ul class="list">
				{% for album in results %}
					<div class="listArtist">
						<div class="wrapper">
							<div class="blog_post">
								<div class="img_pod">
									<li class="list__li"><img src="{{ album.album.image.1['#text']}}" alt="jacket de {{ album.album.name }}"></li>
								</div>
								<div class="container_copy">
									<li class="list__li">{{ album.album.artist }}</li>
									<li class="list__li">{{ album.album.name }}</li>
								</div>
							</div>
						</div>

						<ol class="tracks">
							{% for track in album.album.tracks.track %}
								<div class="listTracks">
									<li><img src="data:image/jpeg;base64," alt="">{{ track.name }}</li>
								</div>
							{% endfor %}
						</ol>
					</div>
                    <div class="transition"></div>

				{% endfor %}
			</ul>
		{% endif %}
	</section>
{% endblock %}
